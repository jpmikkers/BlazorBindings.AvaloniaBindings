<StackPanel>
    <Label>Text</Label>
</StackPanel>

@code
{
    public static void ValidateContent(Avalonia.Visual content)
    {
        var stackPanel = content as AC.StackPanel;
        Assert.That(stackPanel, Is.Not.Null);

        var label = stackPanel.Children.FirstOrDefault() as AC.Label;
        Assert.That(label, Is.Not.Null);
        Assert.That(label.Content, Is.EqualTo("Text"));
    }
}